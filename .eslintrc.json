{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "settings": {
        "import/core-modules": ["google-maps", "dayjs", "dayjs/plugin/advancedFormat", "dotenv-webpack"]
    },
    "rules": {
        "prefer-object-spread": 0
    }
}